 #include<studio.h>
 int main ()
{
int choice; 
printf("please pick up your choice:\n1.Pasta,Rs179\n2.Pizza,Rs239\n3.Burger,Rs149\n4.sandwich,Rs129\n5.French fries,Rs99");
scanf("%d",&choice);
switch (choice)
{case 1: 
printf("Food item-Pasta\nPrice-Rs179");
break;
case 2:
printf("Food item-Pizza\nPrice-Rs239");
break;
case 3:
printf("Food item-Burger\nPrice-Rs149");
break;
printf("Food item-sandwich\nPrice-Rs129");
break;
case 5:
printf("Food item-French fries\nPrice-Rs99");
break;
default:
printf("invalid choice");
}
return 0;
}
